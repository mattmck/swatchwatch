{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      },
	"logLevel": {
      "default": "Trace",
      "Function": "Trace",
      "Host.Aggregator": "Trace"
    	}
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "http": {
      "cors": {
        "supportCredentials": false,
        "allowedOrigins": [
          "http://localhost:3000",
          "http://localhost:3001",
          "https://*.azurestaticapps.net"
        ]
      }
    }
  }
}
